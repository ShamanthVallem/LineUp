{
    "name": "LineUp",
    "version": "1.0.0",
    "description": "This is a best tool to make your day productive",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "permissions": ["storage"],
    "action": {
        "default_title": "Created by Shamanth",
        "default_popup": "popup.html"
    }
}